{
  "name": "@gsmlg/varian",
  "version": "0.1.0",
  "description": "Varian is a Validate library for pure data",
  "repository": "gsmlg/react-suit",
  "license": "MIT",
  "authors": [
    "gsmlg"
  ],
  "files": [
    "es",
    "index.js",
    "umd"
  ],
  "main": "index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "node ./tools/build.js",
    "watch": "babel ./modules -d . --ignore __tests__ --watch",
    "prepublishOnly": "node ./tools/build.js",
    "clean": "git clean -fdX .",
    "lint": "eslint modules",
    "test": "jest"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "coffeescript": "^2.3.1",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.7.0",
    "gzip-size": "^4.1.0",
    "jest": "^22.4.3",
    "jest-preset-coffeescript": "^1.0.0",
    "pretty-bytes": "^4.0.2",
    "raf": "^3.4.0",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "coffee",
      "litcoffee",
      "coffee.md"
    ],
    "transform": {
      ".*": "<rootDir>/tools/test-preprocessor.js"
    },
    "testRegex": "modules/.*\\.(coffee|litcoffee|coffee.md)$",
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": []
}
