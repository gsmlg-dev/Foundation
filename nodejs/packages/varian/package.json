{
  "name": "@gsmlg/varian",
  "version": "0.1.0",
  "description": "Varian is a Validate library for pure data",
  "license": "MIT",
  "authors": [
    "gsmlg"
  ],
  "files": [
    "es",
    "index.js",
    "umd"
  ],
  "main": "index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "node ./tools/build.js",
    "watch": "babel ./modules -d . --ignore __tests__ --watch",
    "prepublishOnly": "node ./tools/build.js",
    "clean": "git clean -fdX .",
    "lint": "eslint modules",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-external-helpers": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@babel/plugin-proposal-do-expressions": "^7.16.0",
    "@babel/plugin-proposal-export-default-from": "^7.16.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.0",
    "@babel/plugin-proposal-function-sent": "^7.16.0",
    "@babel/plugin-proposal-json-strings": "^7.16.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.16.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.0",
    "@babel/plugin-proposal-numeric-separator": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.16.0",
    "@babel/plugin-proposal-throw-expressions": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.16.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^27.3.1",
    "babel-plugin-dev-expression": "^0.2.3",
    "coffeescript": "^2.6.1",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.3",
    "gzip-size": "^6.0.0",
    "jest": "^27.3.1",
    "pretty-bytes": "^5.6.0",
    "raf": "^3.4.1",
    "rollup": "^2.60.0",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0"
  },
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "coffee",
      "litcoffee",
      "coffee.md"
    ],
    "transform": {
      ".*": "<rootDir>/tools/test-preprocessor.js"
    },
    "testRegex": "modules/.*\\.(coffee|litcoffee|coffee.md)$",
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gsmlg/Foundation"
  },
  "bugs": {
    "url": "https://github.com/gsmlg/Foundation/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": []
}
