{
  "name": "@gsmlg/varian",
  "version": "0.1.0",
  "description": "Varian is a Validate library for pure data",
  "license": "MIT",
  "authors": [
    "gsmlg"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "main": "dist/index.js",
  "sideEffects": false,
  "scripts": {
    "test": "gsmlg-scripts test",
    "build": "gsmlg-scripts build",
    "lint": "gsmlg-scripts lint",
    "format": "gsmlg-scripts format",
    "validate": "gsmlg-scripts validate"
  },
  "devDependencies": {
    "@gsmlg/scripts": "^1.7.1",
    "coffeescript": "^2.6.1"
  },
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  },
  "eslintConfig": {
    "extends": [
      "gsmlg",
      "gsmlg/jest"
    ],
    "rules": {
      "no-process-exit": "off",
      "import/no-dynamic-require": "off",
      "import/no-unassigned-import": "off",
      "no-console": "off",
      "no-nested-ternary": "off",
      "no-useless-catch": "off"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gsmlg-dev/Foundation"
  },
  "bugs": {
    "url": "https://github.com/gsmlg-dev/Foundation/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "dependencies": {
    "@babel/runtime": "^7.16.3"
  }
}
